    }
    div.warning::before {
      background-image: url("data:image/svg+xml,%3Csvg class='AlertIcon-sc-1uve1p1-0 eamFBs' xmlns='http://www.w3.org/2000/svg' version='1.1' id='Layer_1' x='0' y='0' viewBox='0 0 500.6 500.6' enable-background='new 0 0 500.6 500.6'%3E%3Cpath d='M250.3 15.1c-132.5 0-239.8 107.2-239.8 239.8 0 132.5 107.2 239.8 239.8 239.8 132.5 0 239.8-107.2 239.8-239.8C490.1 122.3 382.8 15.1 250.3 15.1zM250.3 457.1C138.5 457.1 48 366.7 48 254.8 48 143 138.5 52.5 250.3 52.5c111.8 0 202.3 90.5 202.3 202.3C452.6 366.7 362.1 457.1 250.3 457.1z'%3E%3C/path%3E%3Cpath d='M250.3 135.8c-10.3 0-18.7 8.4-18.7 18.7v120.6c0 10.3 8.4 18.7 18.7 18.7s18.7-8.4 18.7-18.7V154.5C269 144.2 260.7 135.8 250.3 135.8z'%3E%3C/path%3E%3Ccircle cx='250.3' cy='342.1' r='25.3'%3E%3C/circle%3E%3C/svg%3E");
    }
    div.danger::before {
      background-image: url("data:image/svg+xml,%3Csvg class='AlertIcon-sc-1uve1p1-0 gKVDaU' xmlns='http://www.w3.org/2000/svg' version='1.1' id='Layer_1' x='0' y='0' viewBox='0 0 500.6 500.6' enable-background='new 0 0 500.6 500.6'%3E%3Cpath d='M250.3 490.1c132.5 0 239.8-107.2 239.8-239.8 0-132.5-107.2-239.8-239.8-239.8 -132.5 0-239.8 107.2-239.8 239.8C10.6 382.8 117.8 490.1 250.3 490.1zM250.3 48c111.8 0 202.3 90.5 202.3 202.3 0 111.8-90.5 202.3-202.3 202.3C138.5 452.6 48 362.1 48 250.3 48 138.5 138.5 48 250.3 48z'%3E%3C/path%3E%3Cpath d='M168.4 332.2c7.3 7.3 19.2 7.3 26.5 0l137.3-137.3c7.3-7.3 7.3-19.2 0-26.5 -7.3-7.3-19.2-7.3-26.5 0L168.4 305.7C161.1 313 161.1 324.9 168.4 332.2z'%3E%3C/path%3E%3Cpath d='M168.4 168.4c-7.3 7.3-7.3 19.2 0 26.5l137.3 137.3c7.3 7.3 19.2 7.3 26.5 0 7.3-7.3 7.3-19.2 0-26.5L194.9 168.4C187.6 161.1 175.7 161.1 168.4 168.4z'%3E%3C/path%3E%3C/svg%3E");
    }

    /* Schema */
    h2 + div > button {
      border-radius: 6px !important;
      background-color: var(--neutral-color-100) !important;
    }
    .operation-type {
      height: 1.2rem!important;
      line-height: 1rem!important;
      border-radius: 4px!important;
      padding: 0.1rem 0.2rem;
      margin-top: 1px!important;
    }
    .property-name {
      font-size: 12px;
      font-weight: 600;
      font-family: monospace;
    }
    div[data-section-id^="operation/"] h5 > span {
      float: right;
    }
    div[data-section-id^="operation/"] td {
      border-bottom: 0;
    }
    div[data-section-id^="operation/"] td > div:has(table) {
      margin-top: 0 !important;
      margin-bottom: 0 !important;
    }
    td[kind="field"] span:first-child::after {
      display: none;
    }

    /* Headings */
    div[data-section-id^="tag/"] h1,
    div[data-section-id^="tag/"] h2,
    div[data-section-id^="tag/"] h3,
    div[data-section-id^="tag/"] h4 {
      font-weight: 600;
    }
    div[data-section-id^="tag/"] h1 {
      font-size: 2.25rem;
      line-height: 2.75rem;
      letter-spacing: -.08rem;
    }
    div[data-section-id^="tag/"] h2 {
      font-size: 1.75rem;
      line-height: 2.25rem;
      letter-spacing: -.05rem;
    }
    div[data-section-id^="tag/"] h3 {
      font-size: 1.25rem;
      line-height: 1.75rem;
      letter-spacing: -.025rem;
    }
    div[data-section-id^="tag/"] h4 {
      color: var(--neutral-color-900);
      font-size: 0.85rem;
      line-height: 1.5rem;
      text-transform: uppercase;
    }

    .http-verb {
      border-radius: 4px;
    }

    /* Sidebar */
    .menu-content {
      border-right: 1px solid var(--neutral-color-300);
      box-shadow:
        rgba(0, 0, 0, 0) 0px 0px 0px 0px,
        rgba(0, 0, 0, 0) 0px 0px 0px 0px,
        rgba(0, 0, 0, 0.1) 0px 4px 6px -1px,
        rgba(0, 0, 0, 0.06) 0px 2px 4px -1px;
    }
    label.-depth0,
    label.-depth1,
    label.-depth2 {
      font-family: Poppins, sans-serif;
      padding: 8px 10px !important;
      margin: 0px 10px !important;
      border-radius: 6px;
    }
    label.-depth0 {
      color: var(--neutral-color-600);
      font-weight: 600;
    }
    label.-depth1:hover,
    label.-depth2:hover,
    label.-depth1.active,
    label.-depth2.active {
      background-color: #fbf8f3;
    }
    label.-depth1.active {
      color: var(--accent-color);
      font-weight: 600;
    }

    /* Code & Diagrams */
    code {
      text-wrap: nowrap !important;
      border-color: transparent !important;
      padding: 1px 5px !important;
    }
    pre {
      border-color: transparent !important;
      border-radius: 6px !important;
    }
    pre > code { text-shadow: none !important; }

    code[class*="language-"] {
      font-size: .75rem !important;
    }

    pre.mermaid {
      color: var(--primary-color) !important;
      font-size: 14px !important;
      font-family: Poppins, sans-serif !important;
      background-color: transparent !important;
      padding: 4px !important;
      text-shadow: none;
    }
    #redoc pre.mermaid .nodeLabel {
      font-weight: 600;
    }
    #redoc pre.mermaid .edgeLabel {
      background-color: #fff;
    }

    /* Search */
    div[role="search"] > i,
    div[role="search"] > .search-icon {
      width: 14px !important;
      height: 42px !important;
      margin: 0 10px !important;
      line-height: 42px !important;
    }
    div[role="search"] > .search-icon > path {
      fill: var(--neutral-color-700) !important;
    }
    div[role="search"] > .search-input {
      line-height: 13px !important;
      font-weight: 400 !important;
      font-family: Poppins, sans-serif !important;
      background-color: #fff !important;
      border: 1px solid var(--neutral-color-500) !important;
      border-radius: 6px !important;
      padding: 10px 16px 10px 32px !important;
    }
    div[role="search"] div[data-role="search:results"] {
      background-color: #fff !important;
    }
  </style>
</head>
<body>
{{{redocHTML}}}
<script type="module">
  import mermaid from 'https://cdn.jsdelivr.net/npm/mermaid@10/+esm';
  mermaid.initialize({});
</script>
</body>
</html>
